<h1>Favorite Pokemon!</h1>
<nav>
    <a href="/">Back to Main</a>
</nav>

<% allFaves.forEach(fave => { %>
    <h2> <a href='/pokemon/<%= fave.name %>'><%= fave.name %></a> </h2>
    <form method="POST" action="/pokemon/<%= fave.name %>/?_method=DELETE">
        <input type="submit" value="delete">
    </form>

<% }) %>